/* The container */
.container_order_check {
	display: block;
	position: relative;
	padding-left: 35px;
	margin-bottom: 12px;
	cursor: pointer;
	font-size: 14px;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
	color: #999999;
	font-weight: 500;
	letter-spacing: 1px;
}

/* Hide the browser's default checkbox */
.container_order_check input {
	position: absolute;
	opacity: 0;
	cursor: pointer;
	height: 0;
	width: 0;
}

/* Create a custom checkbox */
.checkmark_order_check {
	position: absolute;
	top: 0;
	left: 0;
	height: 25px;
	width: 25px;
	background-color: #eee;
}

/* On mouse-over, add a grey background color */
.container_order_check:hover input~.checkmark_order_check {
	background-color: #ccc;
}

/* When the checkbox is checked, add a blue background */
.container_order_check input:checked~.checkmark_order_check {
	background-color: #262626;
	border-radius: 3px;
}

/* Create the checkmark/indicator (hidden when not checked) */
.checkmark_order_check:after {
	content: "";
	position: absolute;
	display: none;
	border: 1px solid;
}

/* Show the checkmark when checked */
.container_order_check input:checked~.checkmark_order_check:after {
	display: block;
}

/* Style the checkmark/indicator */
.container_order_check .checkmark_order_check:after {
	left: 9px;
	top: 5px;
	width: 5px;
	height: 12px;
	border: solid white;
	border-width: 0 3px 3px 0;
