/*---check----*/

/* The container */
.container_checkbx {
	display: block;
	position: relative;
	margin-bottom: 10px;
	cursor: pointer;
	font-size: 16px;
	color: #8c8c8c;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
	font-weight: 300;
	letter-spacing: 1px;
}

/* Hide the browser's default checkbox */
.container_checkbx input {
	position: absolute;
	opacity: 0;
	cursor: pointer;
	height: 0;
	width: 0;
}

/* Create a custom checkbox */
.checkmark_checkbx {
	position: absolute;
	top: 5px;
	right: 10%;
	height: 16px;
	width: 16px;
	background-color: #fff;
	border: 1px solid #a7a6a6;
}

/* Create the checkmark/indicator (hidden when not checked) */
.checkmark_checkbx:after {
	content: "";
	position: absolute;
	display: none;
}

/* Show the checkmark when checked */
.container_checkbx input:checked~.checkmark_checkbx:after {
	display: block;
}

/* Style the checkmark/indicator */
.container_checkbx .checkmark_checkbx:after {
	left: 6px;
	top: 1px;
	width: 5px;
	height: 10px;
	border: solid #38b861;
	border-width: 0 3px 3px 0;
	-webkit-transform: rotate(45deg);
	-ms-transform: rotate(45deg);
	transform: rotate(45deg);
}
