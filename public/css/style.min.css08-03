/*--rdo--*/

/* The container */
.container_order {
	position: relative;
	padding-left: 35px;
	margin-bottom: 12px;
	cursor: pointer;
	font-size: 16px;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
	color: #585858;
	font-weight: 500;
	letter-spacing: 1px;
}

/* Hide the browser's default radio button */
.container_order input {
	position: absolute;
	opacity: 0;
	cursor: pointer;
}

/* Create a custom radio button */
.checkmark_order {
	position: absolute;
	top: 0;
	left: 0;
	height: 20px;
	width: 20px;
	background-color: #eee;
	border-radius: 50%;
	border: 1px solid #afaeae;
}

/* On mouse-over, add a grey background color */
.container_order:hover input~.checkmark_order {
	background-color: #ccc;
}

/* When the radio button is checked, add a blue background */
.container_order input:checked~.checkmark_order {
	background-color: #262626;
}

/* Create the indicator (the dot/circle - hidden when not checked) */
.checkmark_order:after {
	content: "";
	position: absolute;
	display: none;
}

/* Show the indicator (dot/circle) when checked */
.container_order input:checked~.checkmark_order:after {
	display: block;
}

/* Style the indicator (dot/circle) */
.container_order .checkmark_order:after {
	top: 5px;
	left: 5px;
	width: 8px;
	height: 8px;
	border-radius: 50%;
	background: white;
}
.order_frm_slct {
